{
	"name": "officialxviid/ci4favicons",
	"description": "Auto generate favicons and HTML tags from a PNG file with CodeIgniter 4.",
	"type": "library",
	"keywords": [
		"favicon",
		"generate",
		"png",
		"ico",
		"codeigniter",
		"codeigniter4"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "XVIID Developer",
			"email": "developer@xviid.net",
			"homepage": "http://www.xviid.net"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"codeigniter4/framework": "^4.1",
		"imagine/imagine": "^1.3"
	},
	"suggest": {
		"ext-imagick": "To use the binary and generate favicon images"
	},
	"bin": [
		"favicon"
	],
	"config": {
		"bin-dir": "bin"
	},
	"autoload": {
		"psr-4": {
			"OfficialXVIID\\CI4Favicon\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"xviid-identifier": {
		"code": "0CoG8kIsmfC",
		"title": "Codeigniter 4 Favicon",
		"name": "CI4Favicon",
		"version": "1.0",
		"url": "https://github.com/officialxviid/ci4favicon",
		"logo": {
			"asci": "◠ᴥ◠   CI4Favicon",
			"base": "/dist/img/logo/base.png",
			"base-text": "/dist/img/logo/base-text.png",
			"text": "/dist/img/logo/text.png",
			"horizontal": "/dist/img/logo/horizontal.png",
			"vertical": "/dist/img/logo/vertical.png"
		},
		"company": {
			"name": "XVIID",
			"email": "support@xviid.net",
			"url": "https://xviid.net"
		},
